{
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "plugins": [
        "babel",
        "prettier"
    ],
    "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": { "jsx": true }
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": "error"
    }
}