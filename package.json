{
  "name": "blog-crm",
  "version": "1.0.0",
  "description": "MERN stack application, Intergrading express, mongoDB, React, Material-ui, React-Admin, and storybook.js.",
  "main": "dist",
  "homepage":"./",
  "scripts": {
    "test": "jest",
    "watch:dev": "nodemon",
    "start": "npm run prod",
    "start:client":"npm run start --prefix client",
    "server": "node server_dist",
    "server:install": "npm install",
    "client:install": "npm install --prefix client",
    "build": "npm-run-all clean transpile build:client",
    "build:client":"npm run build --prefix client",
    "install": "npm run server:install && npm run client:install",
    "dev": "concurrently \"npm run dev:server\" \"npm run start:client\"",
    "dev:server": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir server_dist",
    "clean": "rimraf server_dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrWittman612/Blog-CRM.git"
  },
  "keywords": [
    "exoress",
    "mongoDB",
    "React",
    "Material-ui",
    "React-Admin",
    "and",
    "storybook.js"
  ],
  "author": "Chad Wittman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrWittman612/Blog-CRM/issues"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
