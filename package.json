{
  "name": "blog-crm",
  "version": "1.0.0",
  "description": "MERN stack application, Integrating express, mongoDB, React, Material-ui, React-Admin, and storybook.js.",
  "main": "server_dist",
  "homepage": "./",
  "scripts": {
    "test": "jest",
    "watch:dev": "nodemon",
    "start": "npm run prod",
    "start:client": "npm run start --prefix client",
    "postinstall": "npm --prefix client install",
    "server": "node .",
    "install:server": "npm install",
    "install:client": "npm install --prefix client",
    "build": "npm-run-all clean transpile build:client",
    "build:client": "npm run build --prefix client",
    "build:server": "npm-run-all clean transpile",
    "dev": "concurrently \"npm run dev:server\" \"npm run start:client\"",
    "dev:server": "NODE_ENV=development npm-run-all build:server server",
    "prod": "NODE_ENV=production npm-run-all build:server server",
    "transpile": "babel ./server --out-dir server_dist",
    "clean": "rimraf server_dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrWittman612/Blog-CRM.git"
  },
  "keywords": [
    "express",
    "mongoDB",
    "React",
    "Material-ui",
    "React-Admin",
    "and",
    "storybook.js"
  ],
  "author": "Chad Wittman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrWittman612/Blog-CRM/issues"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.13",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
